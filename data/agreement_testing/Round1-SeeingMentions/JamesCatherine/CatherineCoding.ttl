                 ca:hasCoder "cgrady" ;
                 ca:appliesCode citec:software_used ;
               ] ;
			  ca:isTargetOf 
               [ rdf:type ca:CodeApplication ;
                 ca:hasCoder "cgrady" ;
             	 ca:appliesCode [ a citec:software_name ; rdfs:label "geNorm" ] ;
