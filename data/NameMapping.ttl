		rdfs:label "BLAST" ;
		bioj:alternative_name "Basic Local Alignment Search Tool (BLAST)" ; 
		bioj:alternative_name "TBLASTN" ; 
		bioj:alternative_name "BLASTP" ; 
		bioj:alternative_name "BLASTX" ; 
		bioj:alternative_name "BLASTN" ; 
.

